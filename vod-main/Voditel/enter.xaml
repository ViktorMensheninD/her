<Window x:Class="Voditel.enter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Voditel"
        mc:Ignorable="d"
        Title="enter" Height="450" Width="800">
    <Grid Background="#FFE4B5">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="enter" Content="Войти как пользователь" Click="enter_Click" Margin="5"/>
            <Button x:Name="enterex" Content="Войти как экзаменатор" Click="enterex_Click" Margin="5"/>
            <Button x:Name="exit" Content="Выход" Click="exit_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
