<Window x:Class="Voditel.entry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Voditel"
        mc:Ignorable="d"
        Title="entry" Height="450" Width="800">
    <Grid Background="#FFE4B5">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBox x:Name="logbox" Margin="5" Width="120" Height="20"/>
            <PasswordBox x:Name="pswbox" Margin="5" Height="20" Width="120"/>
            <Button x:Name="entry" Content="Вход" Width="50" Height="20" Margin="5"/>
            <Button x:Name="exit" Content="Выход"  Width="50" Height="20" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
